	Keywords = {attitudes; Great Barrier Reef; recreational fishing; zoning; PROTECTED AREAS; NEW-ZEALAND; CONSERVATION; RESERVES; Environmental Sciences & Ecology},
	Language = {English},
	Month = {SEP},
	Note = {PT: J; NR: 23; TC: 2; J9: ENVIRON CONSERV; PG: 8; GA: 573GQ; UT: WOS:000275897300009},
	Number = {3},
	Pages = {245-252},
	Title = {Recreational fishers' attitudes towards the 2004 rezoning of the Great Barrier Reef Marine Park},
	Volume = {36},
