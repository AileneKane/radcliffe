	Keywords = {Acer negundo; Aesculus glabra; Spring greenup; Lonicera maackii; Leafing phenology; Urban heat island; INVASIVE SHRUB; LONICERA-MAACKII; LEAF PHENOLOGY; AMUR HONEYSUCKLE; TREE; PHENOLOGY; CLIMATE-CHANGE; UNITED-STATES; RURAL-AREAS; URBANIZATION; CONSERVATION; Biodiversity & Conservation; Environmental Sciences & Ecology},
	Language = {English},
	Month = {JUN},
	Note = {PT: J; NR: 58; TC: 11; J9: BIOL INVASIONS; PG: 15; GA: 448CC; UT: WOS:000266238700014},
