	Keywords = {Betula pubescens ssp tortuosa; climate warming; low-alpine; Picea abies; Pinus sylvestris; Sorbus aucuparia; species-limit; upward plant migration; PUBESCENS SSP TORTUOSA; MOUNTAIN NATIONAL-PARK; FOREST-TUNDRA ECOTONE; CLIMATIC-CHANGE; CENTRAL SWEDEN; ENVIRONMENTAL-CHANGE; NORTHERN-HEMISPHERE; AIR-TEMPERATURE; EARLY HOLOCENE; PICEA-ABIES; Plant Sciences; Environmental Sciences & Ecology},
	Language = {English},
	Month = {FEB},
	Note = {PT: J; NR: 72; TC: 173; J9: J ECOL; PG: 10; GA: 526XE; UT: WOS:000174154500007},
