	Keywords = {Community ecology; ecological networks; global change; guild assembly; multidimensional complexity; niche theory; prediction; species distribution model; species interactions; trait-based community modules; FOOD WEBS; CLIMATE-CHANGE; SPECIES DISTRIBUTIONS; MUTUALISTIC NETWORKS; POLLINATION NETWORK; FUTURE DISTRIBUTION; IMPROVE; VEGETATION MODEL; HABITAT MODELS; Environmental Sciences & Ecology; Physical Geography},
	Language = {English},
