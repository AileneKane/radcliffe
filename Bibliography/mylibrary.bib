	Keywords = {Vulnerability; Aquatic insects; Bio-ecological traits; Climate change; STREAM MACROINVERTEBRATE COMMUNITIES; FRESH-WATER BIODIVERSITY; SPECIES; TRAITS; HABITAT FRAGMENTATION; DISTRIBUTION PATTERNS; INVERTEBRATE; PLECOPTERA INSECTA; EGG DEVELOPMENT; RUNNING WATERS; RIVER; SYSTEMS; Marine & Freshwater Biology},
	Language = {English},
