	Keywords = {biodiversity; deduction; induction; machine learning; science; BIRD COMMUNITIES; HOUSE FINCHES; DYNAMICS; DISEASE; POPULATION; CHAOS; TIME; Life Sciences & Biomedicine - Other Topics},
	Language = {English},
