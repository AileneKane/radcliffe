	Keywords = {adaptive responses; energy allocation; growing season; life history; optimal control theory; phenology; PHENOLOGICAL RESPONSE; XANTHIUM-CANADENSE; LIFE-HISTORY; POLLINATOR; INTERACTIONS; REPRODUCTIVE GROWTH; FROST DAMAGE; SEED; MASS; PATTERNS; POPULATIONS; Biodiversity & Conservation; Environmental Sciences & Ecology},
	Language = {English},
