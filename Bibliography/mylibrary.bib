	Isbn = {0165-0009 ER},
	Journal = {Climatic Change},
	Note = {PT: J; TC: 20},
	Number = {1-2},
	Pages = {263-282},
	Title = {Taking the pulse of mountains: Ecosystem responses to climatic variability},
	Volume = {59},
	Year = {2003}}

