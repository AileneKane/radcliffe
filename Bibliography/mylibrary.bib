	Journal = {Environmental Biology of Fishes},
	Keywords = {Citizen science; Elasmobranch monitoring; Yellow stingray; Diver survey; Population trend; Trophic interactions; SEAGRASS THALASSIA-TESTUDINUM; MARINE PROTECTED AREAS; REGION-WIDE; DECLINES; CORAL-REEF ECOSYSTEM; GULF-OF-MEXICO; FLORIDA-KEYS; LONG-TERM; UROLOPHUS-JAMAICENSIS; UROBATIS-JAMAICENSIS; SOUTHEAST FLORIDA; Environmental Sciences & Ecology; Marine & Freshwater Biology},
