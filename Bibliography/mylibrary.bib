	Note = {PT: J; NR: 94; TC: 8; J9: RESTOR ECOL; PG: 8; GA: 377TJ; UT: WOS:000261273200015},
	Number = {4},
	Pages = {642-649},
	Title = {The Nature of Urban Soils and Their Role in Ecological Restoration in Cities},
	Volume = {16},
